{
  "name": "slu-reuse",
  "version": "1.0.0",
  "description": "a web app for items donation within Saint Louis University campus",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
